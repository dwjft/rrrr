{
  "presets": ["react", ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]],
  "plugins": [
    ["easy-import", {
      "directories": ["src", "test"]
    }],
    "transform-object-rest-spread",
    "transform-es2015-modules-commonjs",
    "babel-plugin-transform-class-properties",
    "transform-react-jsx-source"
  ]
}
